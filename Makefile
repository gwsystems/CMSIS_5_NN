cifar10: ${C_SRC}
	clang -std=c99 -DARM_MATH_CM3 -ICMSIS_5/CMSIS/DSP/Include -ICMSIS_5/CMSIS/Core/Include -ICMSIS_5/CMSIS/NN/Include -O3 -g3 -Wall -fmessage-length=0 -Wno-unused-function -Wno-unused-variable -o cifar10.out ${C_SRC}

cifar10.wasm: ${C_SRC}
	${WASM_CC} ${WASMCFLAGS} ${OPTFLAGS} -std=c99 -DARM_MATH_CM3 -ICMSIS_5/CMSIS/DSP/Include -ICMSIS_5/CMSIS/Core/Include -ICMSIS_5/CMSIS/NN/Include -O3 -g3 -Wall -fmessage-length=0 -Wno-unused-function -Wno-unused-variable ${C_SRC} ${DUMMY} -o cifar10.wasm 

cifar10.out: cifar10.wasm
	${AWSM_CC} cifar10.wasm -o cifar10_out.bc
	${NATIVE_CC} ${NATIVE_CC_CFLAGS} ${OPTFLAGS} -DUSE_MEM_VM cifar10_out.bc ${AWSM_RT_MEMC} ${AWSM_RT_LIBC} ${AWSM_RT_ENV} ${AWSM_RT_RT} -o cifar10.out

cifar10.so: cifar10.wasm
	${AWSM_CC} --inline-constant-globals --runtime-globals cifar10.wasm -o cifar10_so.bc
	${NATIVE_CC} --shared -fPIC ${NATIVE_CC_CFLAGS} ${OPTFLAGS} -DUSE_MEM_VM -I${SLEDGE_RT_INC} cifar10_so.bc ${SLEDGE_MEMC} ${WASMISA} -o cifar10.so

# cifar10.wasi: ${C_SRC}
# 	${WCC} ${WCFLAGS} -std=c99 -DARM_MATH_CM3 -ICMSIS_5/CMSIS/DSP/Include -ICMSIS_5/CMSIS/Core/Include -ICMSIS_5/CMSIS/NN/Include -O3 -g3 -Wall -fmessage-length=0 -Wno-unused-function -Wno-unused-variable ${C_SRC} -Wl,--allow-undefined,-z,stack-size=524288,--no-threads,--stack-first,--no-entry,--export-all,--export=main --sysroot=${WSYSROOT} -o cifar10.wasi.out


clean:
	rm -f *.wasm *.bc *.out *.so

all: clean cifar10 cifar10.out cifar10.so

.PHONY: cifar10 cifar10.out cifar10.so clean all
